<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#daeae7"
    tools:context=".AddActivitySerOrPost"
    tools:layout_editor_absoluteX="-8dp"
    tools:layout_editor_absoluteY="-124dp">


    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="65dp"
        android:layout_height="54dp"
        android:layout_marginStart="9dp"
        android:layout_marginEnd="337dp"
        android:layout_marginBottom="644dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/baseline_shopping_cart_24" />


    <ImageView
        android:id="@+id/service"
        android:layout_width="65dp"
        android:layout_height="54dp"
        android:layout_marginStart="9dp"
        android:layout_marginEnd="337dp"
        android:layout_marginBottom="576dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/baseline_home_repair_service_24" />

    <Button
        android:id="@+id/sellbut"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="39dp"
        android:layout_marginEnd="208dp"
        android:layout_marginBottom="20dp"
        android:background="?attr/selectableItemBackground"
        android:text="Sell"
        android:textColor="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/servicebut"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/imageView3" />

    <Button
        android:id="@+id/servicebut"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="33dp"
        android:layout_marginEnd="213dp"
        android:layout_marginBottom="580dp"
        android:background="?attr/selectableItemBackground"
        android:text="@string/service"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/service" />
</androidx.constraintlayout.widget.ConstraintLayout>